<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sample.springboot.mybatis.hybrid.service.MemberXmlMapper">
	<select id="findAll" resultType="sample.springboot.mybatis.hybrid.service.Member">
		SELECT id, name, phone FROM MEMBER where name = 'xml'
	</select>
</mapper>

